<%- include('partials/header') -%>
<section class="content">
  <div class="user-info">
    <!-- <h2><%= user.userName %></h2> -->
    <h2>@watch-this</h2>
  </div>
  <div class="gallery u-grid">
    <% for(var i=0; i<posts.length; i++) {%>
    <a href="/post/<%= posts[i]._id%>" class="gallery__link">
      <figure class="gallery__thumb">
        <img src="<%= posts[i].image%>" class="gallery__image">
      </figure>
    </a>
    <% } %>
  </div>
</section>
<%- include('partials/footer') -%>